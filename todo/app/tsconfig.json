{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "lib": [
            "es5",
            "es2015",
            "es2017",
            "dom",
            "scripthost"
        ],
        "noImplicitAny": false,
        "suppressImplicitAnyIndexErrors": true
    },
    "exclude": ["node_modules"]
}
